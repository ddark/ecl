+                if (!bot)
+                    return;
+
+                Group* group = bot->GetGroup();
+                if (!group)
+                    return;
+
+                group->CountRollVote(bot, Guid, NumberOfPlayers, RollVote(choice));
+            }
+            return;
+        }
+        case CMSG_REPAIR_ITEM:
